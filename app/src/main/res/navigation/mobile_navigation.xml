<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_profile">

    <fragment
        android:id="@+id/navigation_profile"
        android:name="com.example.mybrary.ui.profil.ProfileFragment"
        android:label="@string/title_profile"
        tools:layout="@layout/fragment_profile" />

    <fragment
        android:id="@+id/navigation_nearby"
        android:name="com.example.mybrary.ui.nearby.NearbyFragment"
        android:label="@string/title_nearby"
        tools:layout="@layout/fragment_list" />

    <fragment
        android:id="@+id/navigation_mywishes"
        android:name="com.example.mybrary.ui.myWishes.MyWishesFragment"
        android:label="@string/title_mywishes"
        tools:layout="@layout/fragment_list" />
    <fragment
        android:id="@+id/navigation_myBooks"
        android:name="com.example.mybrary.ui.myBooks.MyBooksFragment"
        android:label="@string/title_myBooks"
        tools:layout="@layout/fragment_list" />
</navigation>